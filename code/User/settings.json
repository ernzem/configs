{
    "editor.minimap.enabled": false,
    "editor.scrollbar.verticalScrollbarSize": 8,
    "editor.scrollbar.horizontalScrollbarSize": 8,
    "editor.fontSize": 14.8,
    "editor.lineHeight": 1.4,
    "workbench.activityBar.location": "hidden",
    "editor.stickyScroll.enabled": false,
    "editor.hover.enabled": false,
    "debug.inlineValues": "on",
    "editor.accessibilitySupport": "off",
    "workbench.startupEditor": "none",
    "editor.suggestOnTriggerCharacters": false,
    "editor.quickSuggestions": {
        "other": "off"
    },
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    // "editor.fontFamily": "JetBrains Mono ExtraLight,Menlo, Monaco, 'Courier New', monospace",
    // "editor.fontFamily": "JetBrains Mono Light,Menlo, Monaco, 'Courier New', monospace",
    "editor.fontFamily": "JetBrains Mono ,Menlo, Monaco, 'Courier New', monospace",
    "editor.guides.indentation": false,
    // "editor.fontWeight": "460",
    "editor.bracketPairColorization.enabled": false,
    "editor.tokenColorCustomizations": {
        "[Default Dark Modern]": {
            // "variables": "#CCCCCC",
            "comments": "#808080",
            // "strings": "#6A9955"
            "strings": "#70aa55"
        },
        "[Default Light Modern]": {
            // "variables": "#101010",
            "comments": "#959595",
            "strings": "#008000",
            "types": "#005c8a",
            "keywords": {
                "foreground": "#7c1386",
                // "fontStyle": "bold"
            },
            "textMateRules": [
                // {
                    // "scope": [
                    //     "entity.name.type.go",
                    //     "storage.type..*.go",
                    // ],
                    // "settings": {
                    //     "foreground": "#005c8a"
                    // }
                // },
                {
                    "scope": [
                        "keyword.function.go",
                        "keyword.type.go",
                        "keyword.struct.go",
                        "keyword.const.go",
                        "keyword.interface.go"
                    ],
                    "settings": {
                        "foreground": "#7c1386"
                    }
                }
            ]
        },
        "[chalice-color-theme-light]": {
            "comments": "#707070",
            // "functions": "#936b1f",
            // "types": "#936b1f",
            // "keywords": "#a626a4"
            "keywords": {
                "fontStyle": "bold"
            }
        }
    },
    "workbench.colorCustomizations": {
        // "[Default Light Modern]": {
        //     "editor.background": "#F0F0F0",
        //     "sideBar.background": "#F0F0F0",
        //     "titleBar.activeBackground": "#F0F0F0",
        //     "statusBar.background":"#F0F0F0"
        // },
        "[chalice-color-theme-light]": {
            //     // // "editor.background": "#F0F0F0",
            //     "sideBar.background": "#F0F0F0",
            //     "sideBar.border": "#dddddd",
            //     "titleBar.activeBackground": "#DDDDDD",
            //     "titleBar.border":"#dddddd",
            //     "editorGroupHeader.tabsBorder": "#dddddd",
            //     "tab.inactiveBackground": "#F0F0F0",
            //     "tab.border": "#dddddd",
            //     "tab.dragAndDropBorder": "#dddddd",
            //     // "tab.activeBorderTop": "#dddddd",
            //     "tab.unfocusedInactiveBackground": "#F0F0F0",
            //     "tab.unfocusedActiveBorder": "#dddddd",
            //     "tab.unfocusedActiveBorderTop": "#dddddd",
            //     "statusBar.background":"#F0F0F0",
            // "statusBarItem.remoteBackground": "#C8C8C8",
            // "statusBarItem.remoteForeground": "#303030"
        },
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript][javascriptreact][typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json][jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[css][scss][less]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "workbench.editorAssociations": {
        "*.md": "vscode.markdown.preview.editor"
    },
    "git.blame.editorDecoration.enabled": false,
    "git.blame.statusBarItem.enabled": true,
    "window.menuBarVisibility": "toggle",
    "diffEditor.hideUnchangedRegions.enabled": true,
    "window.title": "${dirty} ${activeEditorMedium}${separator}${folderName}",
    "git.autofetch": true,
    "keyboard.dispatch": "keyCode",
    "go.toolsManagement.autoUpdate": true,
    "git.decorations.enabled": false,
    "numberedBookmarks.gutterIconFillColor": "#3452b4",
    "numberedBookmarks.gutterIconNumberColor": "#f2f2f2",
    "workbench.editor.editorActionsLocation": "hidden",
    "chat.commandCenter.enabled": false,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "window.titleBarStyle": "native",
    "git.openRepositoryInParentFolders": "never",
    "editor.parameterHints.enabled": false,
    "workbench.iconTheme": "chalice-icon-theme",
    "breadcrumbs.enabled": false,
    "workbench.colorTheme": "Default Light Modern"
}