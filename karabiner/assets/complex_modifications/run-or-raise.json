{
    "title": "Run or Raise Apps",
    "rules": [
        {
            "description": "left_option + w  -> Brave",
            "manipulators": [
                {
                    "from": {
                        "key_code": "w",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Brave Browser.app'"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "left_option + s  -> VSCode",
            "manipulators": [
                {
                    "from": {
                        "key_code": "s",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Visual Studio Code.app'"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "left_option + shift + s  -> IntelliJ IDE CE",
            "manipulators": [
                {
                    "from": {
                        "key_code": "s",
                        "modifiers": {
                            "mandatory": [
                                "left_option",
                                "left_shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/IntelliJ IDEA CE.app'"
                        }
                    ],
