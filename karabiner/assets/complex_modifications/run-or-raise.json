{
  "title": "Run or Raise Apps",
  "rules": [
    {
      "description": "left_option + n  -> VSCodium",
      "manipulators": [
        {
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": [
                "left_option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/VSCodium.app'"
            }
          ],
          "type": "basic"
        }
      ]
    },
